{
  "name": "snap-shot-vue-test",
  "version": "1.0.0",
  "description": "Jest snapshot testing for Vue.js project using snap-shot",
  "main": "index.js",
  "scripts": {
    "test": "jest --no-cache"
  },
  "keywords": ["Jest", "snapshot", "vue", "vue.js"],
  "private": true,
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/snap-shot-vue-test/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahmutov/snap-shot-vue-test.git"
  },
  "homepage": "https://github.com/bahmutov/snap-shot-vue-test#readme",
  "devDependencies": {
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "^6.22.0",
    "jest": "^18.1.0",
    "snap-shot": "^2.7.1",
    "vue": "^2.1.10",
    "vue-loader": "^10.3.0",
    "vue-server-renderer": "^2.1.10",
    "vue-template-compiler": "^2.1.10"
  },
  "jest": {
    "moduleNameMapper": {
       "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "<rootDir>/vue-transform.js",
      "^.+\\.js$": "babel-jest"
    }
  }
}
